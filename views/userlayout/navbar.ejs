<!-- Header start -->
<header class="ec-header">
    <!-- Ec Header Top Start -->
    <div class="header-top">
        <div class="container">
            <!-- Header Top responsive Action -->
            <div class="row d-lg-none">
                <div class="col">
                    <div class="ec-header-bottons">
                        <!-- Header User Start -->
                        <div class="ec-header-user dropdown">
                            <button class="dropdown-toggle" data-bs-toggle="dropdown">
                                <img src="/assets/images/icons/user.svg" class="svg_img header_svg" alt="" />
                            </button>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <% if(session == null) { %>
                                <li><a class="dropdown-item" href="/register">Register</a></li>
                                <li><a class="dropdown-item" href="/login">Login</a></li>
                                <% }else{ %>
                                <li><a class="dropdown-item" href="/user-profile">My Profile</a></li>
                                <li><a class="dropdown-item" href="/logout">Logout</a></li>
                                <% } %>
                            </ul>
                        </div>
                        <!-- Header User End -->
                        <!-- Header Cart Start -->
                        <a href="/wishlist-page" class="ec-header-btn ec-header-wishlist">
                            <div class="header-icon">
                                <img src="/assets/images/icons/wishlist.svg" class="svg_img header_svg" alt="" />
                            </div>
                        </a>
                        <!-- Header Cart End -->
                        <!-- Header Cart Start -->
                        <a href="/cart-page" class="ec-header-btn ec-side-toggle">
                            <div class="header-icon">
                                <img src="/assets/images/icons/cart.svg" class="svg_img header_svg" alt="" />
                            </div>
                        </a>
                        <!-- Header Cart End -->
                        <a href="javascript:void(0)" class="ec-header-btn ec-sidebar-toggle">
                            <img src="/assets/images/icons/category-icon.svg" class="svg_img header_svg" alt="icon" />
                        </a>
                        <!-- Header menu Start -->
                        <a href="#ec-mobile-menu" class="ec-header-btn ec-side-toggle">
                            <img src="/assets/images/icons/menu.svg" class="svg_img header_svg" alt="icon" />
                        </a>
                        <!-- Header menu End -->
                    </div>
                </div>
            </div>
            <!-- Header Top responsive Action -->
        </div>
    </div>
    <!-- Ec Header Top  End -->
    <!-- Ec Header Bottom  Start -->
    <div class="ec-header-bottom">
        <div class="container position-relative">
            <div class="row">
                <!-- Ec Header Logo Start -->
                <div class="col-lg-3">
                    <div class="header-logo">
                        <h1>ESSENCE</h1>
                    </div>
                </div>
                <!-- Ec Header Logo End -->
                <!-- Ec Header Menu Start -->
                <div class="col-lg-6">
                    <nav class="ec-main-menu">
                        <ul class="ec-menu-list">
                            <li><a href="/home-page">Home</a></li>
                            <li><a href="/shop-page">Shop</a></li>
                        </ul>
                    </nav>
                </div>
                <!-- Ec Header Menu End -->
                <!-- Ec Header Button Start -->
                <div class="col-lg-3">
                    <div class="ec-header-bottons">
                        <!-- Header User Start -->
                        <div class="ec-header-user dropdown">
                            <button class="dropdown-toggle" data-bs-toggle="dropdown">
                                <img src="/assets/images/icons/user.svg" class="svg_img header_svg" alt="" />
                            </button>
                            <ul class="dropdown-menu dropdown-menu-right">
                                <% if(session == null) { %>
                                <li><a class="dropdown-item" href="/register">Register</a></li>
                                <li><a class="dropdown-item" href="/login">Login</a></li>
                                <% }else{ %>
                                <li><a class="dropdown-item" href="/user-profile">My Profile</a></li>
                                <li><a class="dropdown-item" href="/logout">Logout</a></li>
                                <% } %>
                            </ul>
                        </div>
                        <!-- Header User End -->
                        <!-- Header wishlist Start -->
                        <a href="/wishlist-page" class="ec-header-btn ec-header-wishlist">
                            <div class="header-icon">
                                <img src="/assets/images/icons/wishlist.svg" class="svg_img header_svg" alt="" />
                            </div>
                        </a>
                        <!-- Header wishlist End -->
                        <!-- Header Cart Start -->
                        <a href="/cart-page" class="ec-header-btn">
                            <div class="header-icon">
                                <img src="/assets/images/icons/cart.svg" class="svg_img header_svg" alt="" />
                            </div>
                        </a>
                        <!-- Header Cart End -->
                    </div>
                </div>
                <!-- Ec Header Button End -->
            </div>
        </div>
    </div>
    <!-- Ec Header Bottom  End -->
    <!-- Header responsive Bottom  Start -->
    <div class="ec-header-bottom d-lg-none">
        <div class="container position-relative">
            <div class="row">
                <!-- Ec Header Logo Start -->
                <div class="col-6">
                    <div class="header-logo">
                        <h1>ESSENCE</h1>
                    </div>
                </div>
                <!-- Ec Header Logo End -->
                <!-- Ec Header Menu Start -->
                <div class="col-6">
                    <nav class="ec-main-menu">
                        <ul class="ec-menu-list">
                            <li><a href="/home-page">Home</a></li>
                            <li><a href="/shop-page">Shop</a></li>
                        </ul>
                    </nav>
                </div>
                <!-- Ec Header Menu End -->
            </div>
        </div>
    </div>
    <!-- Header responsive Bottom  End -->
</header>
<!-- EC Main Menu Start -->
